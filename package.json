{
  "name": "ai-agent-flow-editor",
  "version": "1.0.0",
  "description": "Visual editor for AI agent flow configurations",
  "scripts": {
    "check-env": "node scripts/check-env.js",
    "dev": "npm run check-env && next dev",
    "build": "npm run check-env && next build",
    "start": "npm run check-env && next start -H 0.0.0.0",
    "start:prod": "NODE_ENV=production npm run check-env && NODE_ENV=production next start -H 0.0.0.0",
    "lint": "next lint"
  },
  "keywords": [
    "ai",
    "agent",
    "flow",
    "editor",
    "canvas"
  ],
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "mobx-react-lite": "^4.1.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  }
}
